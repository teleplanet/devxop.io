   .login_layout{
    width: 100%;
    height: 100%;
    position: absolute;
  }
  
  .loginscreen.screen-split {
      -webkit-box-align: stretch;
      -webkit-align-items: stretch;
      -ms-flex-align: stretch;
      align-items: stretch;
      -webkit-flex-flow: row nowrap;
      -ms-flex-flow: row nowrap;
      flex-flow: row nowrap;
      height: 100vh;
      -webkit-box-pack: center;
      -webkit-justify-content: center;
      -ms-flex-pack: center;
      justify-content: center;
      left: 0;
      position: absolute;
      right: 0;
      top: 0;
      width: 100vw;
  }
  
  .loginscreen {
      -webkit-box-align: center;
      -webkit-align-items: center;
      -ms-flex-align: center;
      align-items: center;
      background-color: #ffffff;
      bottom: 0;
      display: -webkit-box;
      display: -webkit-flex;
      display: -ms-flexbox;
      display: flex;
      -webkit-flex-flow: column nowrap;
      -ms-flex-flow: column nowrap;
      flex-flow: column nowrap;
      font-size: 16px;
      height: 100vh;
      -webkit-box-pack: center;
      -webkit-justify-content: center;
      -ms-flex-pack: center;
      justify-content: center;
      left: 0;
      position: absolute;
      right: 0;
      top: 0;
      width: 100vw;
  }
  
  .loginscreen-pane {
      display: -webkit-box;
      display: -webkit-flex;
      display: -ms-flexbox;
      display: flex;
      -webkit-flex-flow: column nowrap;
      -ms-flex-flow: column nowrap;
      flex-flow: column nowrap;
      -webkit-box-pack: center;
      -webkit-justify-content: center;
      -ms-flex-pack: center;
      justify-content: center;
  }
  
  .loginscreen-pane.pane-form {
    margin: initial;
      max-width: 520px;
      min-width: 480px;
  }
  
  .loginscreen.screen-split .loginscreen-pane {
      -webkit-box-flex: 1;
      -webkit-flex: 1;
      -ms-flex: 1;
      flex: 1;
  }
  
  .loginscreen-pane.pane-artwork {
      -webkit-box-align: center;
      -webkit-align-items: center;
      -ms-flex-align: center;
      align-items: center;
      -webkit-box-pack: center;
      -webkit-justify-content: center;
      -ms-flex-pack: center;
      justify-content: center;
      background-color: #2e2f30;
      overflow: hidden;
  }
  
  .artwork_image{
    background-position: 50% 50%;
    background-repeat: no-repeat;
    background-size: 100% auto;
    height: 40vw;
    max-width: 500px;
    width: 30vw;
  }
  
  .login-page{
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: stretch;
    -webkit-align-items: stretch;
    -ms-flex-align: stretch;
    align-items: stretch;
    -webkit-flex-flow: column nowrap;
    -ms-flex-flow: column nowrap;
    flex-flow: column nowrap;
    padding: 0 48px;
    overflow-x: auto;
    -webkit-overflow-scrolling: touch;
    overflow-x: hidden;
    overflow-y: auto;
  }
  
  .login-header{
    text-align: center;
    margin-bottom: 10px;
  }
  
  
  .submit{
    padding: 10px;
    padding-right: 20px;
    padding-left: 20px;
    background-color: #ccc;
    color: #fff;
    border-radius: 20px;
    border: none;
    font-size: 18px;
    font-weight: 300;
    padding: 8px 24px;
    -webkit-transition: background-color .2s ease-in-out;
    -moz-transition: background-color .2s ease-in-out;
    transition: background-color .2s ease-in-out;
  }
  
  .submit:hover{
    background-color: #19aec2;
  }
  
  .input-box{
    width: 100%;
    height: auto;
  }
  
  .input-box-checkbox{
    width: 100%;
    height: auto;
  }
  
  .input-box-image{
    width: 100%;
    height: auto;
    height: 80px;
    margin-bottom: 20px;
  }
  
  .input-image{
    width: 100%;
    height: auto;
    padding-top: 20px;
    margin-top: 20px;
    text-align: center;
  }
  
  .placeholder{
    position: relative;
    padding-top: 25px;
    color: #777a7c;
    width: 100%;
    bottom: 0px;
    height: auto;
    font-size: 18px;
    -webkit-transform: scale(1);
    -ms-transform: scale(1);
    transform: scale(1);
    will-change: transform;
    -webkit-transition:font-size .2s cubic-bezier(.4,0,.2,1), bottom .2s cubic-bezier(.4,0,.2,1),padding-top .2s cubic-bezier(.4,0,.2,1), height .2s cubic-bezier(.4,0,.2,1);
    transition: bottom .2s cubic-bezier(.4,0,.2,1),
      height .2s cubic-bezier(.4,0,.2,1),
      font-size .2s cubic-bezier(.4,0,.2,1),
      padding-top .2s cubic-bezier(.4,0,.2,1);
  }
  
  .initial{
    cursor: pointer;
    padding-top: 0px !important;
    height: 50px !important;
    bottom: -50px !important;
    font-size: 24px !important;
    
  }
  
